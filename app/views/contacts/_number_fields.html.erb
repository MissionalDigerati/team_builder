<fieldset class="attributes">
	<%= f.label :phone_type, "Phone Type" %>
	<%= f.select :phone_type, Number::TYPES, :include_blank => true, :require => true %><br/><br/>
	<%= f.label :number, "Phone Number" %>
	<%= f.text_field :number %><br/><br/>
	<%= f.hidden_field :_destroy %>
	<%= link_to "remove", '#', class: "remove_fields btn btn-small btn-info" %>
	<br/>
	<br/>
</fieldset>	