<%= form_for([@presence]) do |f| %>
	<%= f.label :contact_id, "Contact" %><br/>
	<%= f.select(:contact_id, Contact.all.collect {|p| [ p.first_name, p.id ] }) %><br/>
	<%= f.label :site, "Site" %><br/>
	<%= f.select :site, Presence::SITES, {:include_blank => true} %><br/>
	<%= f.label :url, "URL" %><br/>
	<%= f.text_field :url, :required => true %><br/>
	<%= f.label :account, "Account" %><br/>
	<%= f.text_field :account, :required => true %><br/>
	<%= f.submit %>
<% end %>


